{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, proxy-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/manifest.webmanifest", "destination": "/manifest-v2.webmanifest" },
    { "source": "/manifest.json", "destination": "/manifest-v2.webmanifest" }
  ]
}
